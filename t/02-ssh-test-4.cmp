---
foo_port: 22
foo_address_family: any
foo_listen_address: ['0.0.0.0', '::']
foo_protocol: 2
foo_host_key: ['/etc/ssh/ssh_host_key', '/etc/ssh/ssh_host_rsa_key', '/etc/ssh/ssh_host_dsa_key', '/etc/ssh/ssh_host_ecdsa_key']
foo_key_regeneration_interval: 1h
foo_server_key_bits: 1024
foo_syslog_facility: AUTH
foo_log_level: INFO
foo_login_grace_time: 2m
foo_permit_root_login: yes
foo_strict_modes: yes
foo_max_auth_tries: 6
foo_max_sessions: 10
foo_rsa_authentication: yes
foo_pubkey_authentication: yes
foo_authorized_keys_file: .ssh/authorized_keys
foo_authorized_principals_file: none
foo_authorized_keys_command: none
foo_authorized_keys_command_user: nobody
foo_rhosts_rsa_authentication: no
foo_hostbased_authentication: no
foo_ignore_user_known_hosts: no
foo_ignore_rhosts: yes
foo_password_authentication: yes
foo_permit_empty_passwords: no
foo_challenge_response_authentication: no
foo_kerberos_authentication: no
foo_kerberos_or_local_passwd: yes
foo_kerberos_ticket_cleanup: yes
foo_kerberos_get_afs_token: no
foo_gssapi_authentication: no
foo_gssapi_cleanup_credentials: yes
foo_use_pam: yes
foo_allow_agent_forwarding: yes
foo_allow_tcp_forwarding: yes
foo_gateway_ports: no
foo_x11_forwarding: no
foo_x11_display_offset: 10
foo_x11_use_localhost: yes
foo_print_motd: no
foo_print_last_log: yes
foo_tcp_keep_alive: yes
foo_use_login: no
foo_use_privilege_separation: sandbox
foo_permit_user_environment: no
foo_compression: delayed
foo_client_alive_interval: 0
foo_client_alive_count_max: 3
foo_use_dns: yes
foo_pid_file: /run/sshd.pid
foo_max_startups: 10:30:100
foo_permit_tunnel: no
foo_chroot_directory: none
foo_version_addendum: none
foo_banner: none
foo_subsystem_sftp: /usr/lib/ssh/sftp-server
